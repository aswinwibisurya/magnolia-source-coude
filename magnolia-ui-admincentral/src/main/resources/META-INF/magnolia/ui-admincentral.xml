  <dependencies>
    <dependency>
      <name>core</name>
      <version>5.0/*</version>
    </dependency>
    <!-- the dependency is just to ensure order of loading of modules -->
    <dependency>
      <name>adminInterface</name>
      <version>5.0/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>cache</name>
      <version>5.0/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>groovy</name>
      <version>1.2.3/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>dms</name>
      <version>1.6.2/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>workflow</name>
      <version>5.0/*</version>
      <optional>true</optional>
    </dependency>
    <dependency>
      <name>demo-project</name>
      <version>1.6/*</version>
      <optional>true</optional>
    </dependency>
  </dependencies>

  <servlets>
    <servlet>
      <name>AdminCentral</name>
      <class>info.magnolia.ui.vaadin.integration.servlet.MultipleBrowserWindowsApplicationServlet</class>
      <mappings>
        <mapping>/.magnolia/admincentral</mapping>
        <mapping>/.magnolia/admincentral/*</mapping>
      </mappings>
      <params>
        <param>
          <name>application</name>
          <value>info.magnolia.ui.admincentral.AdminCentralApplication</value>
        </param>
        <param>
          <name>widgetset</name>
          <value>info.magnolia.ui.vaadin.widgetset.MagnoliaWidgetSet</value>
        </param>
        <param>
          <name>theme</name>
          <value>admincentraltheme</value>
        </param>
        <param>
          <name>codeBasePath</name>
          <value>/.magnolia/admincentral</value>
        </param>
      </params>
    </servlet>
    <servlet>
      <name>VaadinClasspathResourcesSpool</name>
      <class>info.magnolia.cms.servlets.ClasspathSpool</class>
      <mappings>
        <mapping>/VAADIN/*</mapping>
      </mappings>
      <params>
        <param>
          <name>resourcesRoot</name>
          <value>/VAADIN</value>
        </param>
      </params>
    </servlet>
  </servlets>

  <repositories>
    <repository>
      <name>magnolia</name>
      <workspaces>
        <workspace>messages</workspace>
      </workspaces>
      <nodeTypeFile>/mgnl-nodetypes/magnolia-message-nodetypes.xml</nodeTypeFile>
    </repository>
  </repositories>
</module>

