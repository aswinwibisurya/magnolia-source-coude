    <component>
      <type>info.magnolia.ui.admincentral.workbench.action.WorkbenchActionFactory</type>
      <implementation>info.magnolia.ui.admincentral.workbench.action.WorkbenchActionFactoryImpl</implementation>
    </component>
    <component>
      <type>info.magnolia.ui.admincentral.content.view.ChooseDialogContentPresenter</type>
      <implementation>info.magnolia.ui.admincentral.content.view.ChooseDialogContentPresenter</implementation>
    </component>
  </components>

  <components>
    <id>subapp</id>
    <configurer>
      <class>info.magnolia.ui.framework.event.SubAppEventBusConfigurer</class>
    </configurer>
  </components>

  <components>
    <id>app-assets</id>
  </components>

  <components>
    <id>app-assets-main</id>
