<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd">
<module>
    <name>ui-pages-app</name>
    <displayName>UI pages app</displayName>
    <description>UI pages app</description>
    <version>${project.version}</version>

    <!-- App components definition. -->
    <!-- id of the components should be app-'appname'. 'appname' is the name of the app configured
         in the apps configuration repository of the admin central. (config/module/apps/appname/name:appname) -->
    <components>
        <id>app-pages</id>
    </components>

  <components>
    <id>app-pages-main</id>
    <component>
      <type>info.magnolia.ui.app.pages.main.PagesMainSubApp</type>
      <implementation>info.magnolia.ui.app.pages.main.PagesMainSubApp</implementation>
    </component>
    <component>
      <type>info.magnolia.ui.app.pages.main.PagesMainView</type>
      <implementation>info.magnolia.ui.app.pages.main.PagesMainViewImpl</implementation>
    </component>
    <component>
      <type>info.magnolia.ui.admincentral.workbench.ContentWorkbenchView</type>
      <implementation>info.magnolia.ui.admincentral.workbench.ContentWorkbenchViewImpl</implementation>
    </component>
    <component>
      <type>info.magnolia.ui.admincentral.workbench.ContentWorkbenchPresenter</type>
      <implementation>info.magnolia.ui.admincentral.workbench.ContentWorkbenchPresenter</implementation>
    </component>
  </components>

  <components>
    <id>app-pages-editor</id>
    <component>
      <type>info.magnolia.ui.app.pages.editor.PagesEditorSubApp</type>
      <implementation>info.magnolia.ui.app.pages.editor.PagesEditorSubApp</implementation>
    </component>
    <component>
      <type>info.magnolia.ui.app.pages.editor.PagesEditorSubAppView</type>
      <implementation>info.magnolia.ui.app.pages.editor.PagesEditorSubAppViewImpl</implementation>
    </component>
    <component>
      <type>info.magnolia.ui.widget.editor.PageEditorView</type>
      <implementation>info.magnolia.ui.widget.editor.PageEditor</implementation>
    </component>
  </components>

  <dependencies>
        <dependency>
            <name>ui-admincentral</name>
            <version>5.0/*</version>
        </dependency>
    </dependencies>
</module>
