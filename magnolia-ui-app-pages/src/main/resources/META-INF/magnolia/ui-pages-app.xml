<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd">
<module>
    <name>ui-pages-app</name>
    <displayName>UI pages app</displayName>
    <description>UI pages app</description>
    <version>${project.version}</version>

    <!-- App components definition. -->
    <!-- id of the components should be app-'appname'. 'appname' is the name of the app configured
         in the apps configuration repository of the admin central. (config/module/apps/appname/name:appname) -->
    <components>
        <id>app-pages</id>
        <component>
            <type>info.magnolia.ui.app.pages.PagesMainSubApp</type>
            <implementation>info.magnolia.ui.app.pages.PagesMainSubApp</implementation>
        </component>
        <component>
            <type>info.magnolia.ui.app.pages.PagesMainView</type>
            <implementation>info.magnolia.ui.app.pages.PagesMainViewImpl</implementation>
        </component>
        <component>
            <type>info.magnolia.ui.app.pages.PageEditorSubApp</type>
            <implementation>info.magnolia.ui.app.pages.PageEditorSubApp</implementation>
        </component>
        <component>
            <type>info.magnolia.ui.app.pages.PageEditorView</type>
            <implementation>info.magnolia.ui.app.pages.PageEditorViewImpl</implementation>
        </component>
        <component>
            <type>info.magnolia.ui.admincentral.workbench.ContentWorkbenchView</type>
            <implementation>info.magnolia.ui.admincentral.workbench.ContentWorkbenchViewImpl</implementation>
        </component>
        <component>
            <type>info.magnolia.ui.admincentral.workbench.ContentWorkbenchSubApp</type>
            <implementation>info.magnolia.ui.admincentral.workbench.ContentWorkbenchSubApp</implementation>
        </component>
    </components>

    <dependencies>
        <dependency>
            <name>ui-admin-central</name>
            <version>5.0/*</version>
        </dependency>
    </dependencies>
</module>
